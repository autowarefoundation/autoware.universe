<launch>
    <arg name="min_segment_length" default="1.5" description="[m]"/>
    <arg name="max_segment_distance" default="30.0" description="[m]"/>
    <arg name="rtk_enabled" default="false" description="GNSS-RTK is enabled?"/>

    <arg name="static_linear_covariance" default="12.00"/>
    <arg name="static_angular_covariance" default="0.0010"/>
    <arg name="static_scale_factor" default="-1.0" description="if negative value, static_scale_factor is not used"/>

    <arg name="road_marking_labels" default="[zebra_marking, line_thin, line_thick, pedestrian_marking, stop_line, road_border]"/>
    <arg name="sign_board_labels" default="[sign-board]"/>

    <include file="$(find-pkg-share pcdless_launch)/launch/impl/run.launch.xml"/>
</launch>