    <arg name="output/objects" value="$(var output_roi_topic)3"/>
    <arg name="output/mask" value="$(var output_mask_topic)3"/>
    <arg name="gpu_id" value="$(var gpu_id_image_raw3)"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='5'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw4)"/>
    <arg name="output/objects" value="$(var output_roi_topic)4"/>
    <arg name="output/mask" value="$(var output_mask_topic)4"/>
    <arg name="gpu_id" value="$(var gpu_id_image_raw4)"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='6'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw5)"/>
    <arg name="output/objects" value="$(var output_roi_topic)5"/>
    <arg name="output/mask" value="$(var output_mask_topic)5"/>
    <arg name="gpu_id" value="$(var gpu_id_image_raw5)"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='7'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw6)"/>
    <arg name="output/objects" value="$(var output_roi_topic)6"/>
    <arg name="output/mask" value="$(var output_mask_topic)6"/>
    <arg name="gpu_id" value="$(var gpu_id_image_raw6)"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='8'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
