  </include>
  <include if="$(eval &quot;'$(var image_number)'>='3'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw2)"/>
