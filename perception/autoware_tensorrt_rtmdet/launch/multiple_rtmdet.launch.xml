  <arg name="image_raw6" default="/sensing/camera/camera6/image_raw"/>
  <arg name="gpu_id_image_raw6" default="0"/>

  <arg name="image_raw7" default="/sensing/camera/camera7/image_raw"/>
  <arg name="gpu_id_image_raw7" default="0"/>

  <arg name="image_number" default="6"/>
  <arg name="output_roi_topic" default="/perception/object_recognition/detection/rois"/>
  <arg name="output_mask_topic" default="/perception/object_recognition/detection/mask"/>

  <include if="$(eval &quot;'$(var image_number)'>='1'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw0)"/>
    <arg name="output/objects" value="$(var output_roi_topic)0"/>
    <arg name="output/mask" value="$(var output_mask_topic)0"/>
    <arg name="gpu_id" value="$(var gpu_id_image_raw0)"/>
  </include>
  <include if="$(eval &quot;'$(var image_number)'>='2'&quot;)" file="$(find-pkg-share autoware_tensorrt_rtmdet)/launch/rtmdet.launch.xml">
    <arg name="input/image" value="$(var image_raw1)"/>
    <arg name="output/objects" value="$(var output_roi_topic)1"/>
    <arg name="output/mask" value="$(var output_mask_topic)1"/>
