{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "autoware_external_velocity_limit_selector parameter",
    "type": "object",
    "definitions": {
      "external_velocity_limit_selector": {
        "type": "object",
        "properties": {
          "max_velocity": {
            "type": "number",
            "description": "Default max velocity [m/s]"
          },
          "normal": {
            "type": "object",
            "properties": {
              "min_acc": {
                "type": "number",
                "description": "Minimum acceleration [m/ss]"
              },
              "max_acc": {
                "type": "number",
                "description": "Maximum acceleration [m/ss]"
              },
              "min_jerk": {
                "type": "number",
                "description": "Minimum jerk [m/sss]"
              },
              "max_jerk": {
                "type": "number",
                "description": "Maximum jerk [m/sss]"
              }
            },
            "required": ["min_acc", "max_acc", "min_jerk", "max_jerk"],
            "additionalProperties": false
          },
          "limit": {
            "type": "object",
            "properties": {
              "min_acc": {
                "type": "number",
                "description": "Minimum acceleration to be observed [m/ss]"
              },
              "max_acc": {
                "type": "number",
                "description": "Maximum acceleration to be observed [m/ss]"
              },
              "min_jerk": {
                "type": "number",
                "description": "Minimum jerk to be observed [m/sss]"
              },
              "max_jerk": {
                "type": "number",
                "description": "Maximum jerk to be observed [m/sss]"
              }
            },
            "required": ["min_acc", "max_acc", "min_jerk", "max_jerk"],
            "additionalProperties": false
          }
        },
        "required": ["max_velocity", "normal", "limit"],
        "additionalProperties": false
      }
    },
    "properties": {
      "/**": {
        "type": "object",
        "properties": {
          "ros__parameters": {
            "$ref": "#/definitions/external_velocity_limit_selector"
          }
        },
        "required": ["ros__parameters"],
        "additionalProperties": false
      }
    },
    "required": ["/**"],
    "additionalProperties": false
  }
  