{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "autoware_pointcloud_preprocesor parameters",
  "type": "object",
  "definitions": {
    "passthrough_filter_uint16_node": {
      "type": "object",
      "properties": {
        "filter_limit_min": {
          "type": "integer",
          "description": "Minimum allowed field value.",
          "default": 0,
          "minimum": 0
        },
        "filter_limit_max": {
          "type": "integer",
          "description": "Maximum allowed field value.",
          "default": 127,
          "minimum": 0
        },
        "filter_field_name": {
          "type": "string",
          "description": "Filtering field name.",
          "default": "channel"
        },
        "keep_organized": {
          "type": "boolean",
          "description": "Flag to keep the indices structure intact after filtering.",
          "default": false
        },
        "filter_limit_negative": {
          "type": "boolean",
          "description": "Flag to determine whether to filter inside or outside the given limits.",
          "default": false
        }
      },
      "required": [
        "filter_limit_min",
        "filter_limit_max",
        "filter_field_name",
        "keep_organized",
        "filter_limit_negative"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "/**": {
      "type": "object",
      "properties": {
        "ros__parameters": {
          "$ref": "#/definitions/passthrough_filter_uint16_node"
        }
      },
      "required": ["ros__parameters"],
      "additionalProperties": false
    }
  },
  "required": ["/**"],
  "additionalProperties": false
}
